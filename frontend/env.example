# AI Agent Self-Onboarding Platform - Frontend Environment Variables

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
NEXT_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token

# Third-party Integrations
NEXT_PUBLIC_CALENDLY_URL=https://calendly.com/your-calendly-link
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Feature Flags
NEXT_PUBLIC_ENABLE_VOICE_BOT=true
NEXT_PUBLIC_ENABLE_SOCIAL_VALIDATION=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# Development
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_MOCK_API=false 